{
  "name": "cattails",
  "version": "1.0.0",
  "description": "Service for working with estuary's feature flags and experiments",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "npx tsx watch src/index.ts",
    "build": "rm -rf dist && tsup --clean --format esm ./src/index.ts",
    "start": "node dist/index.js",
    "test": "npx vitest",
    "testData": "./mongodb/init-db.sh && npx tsx ./src/testing/insertTestData.ts",
    "coverage": "npx vitest run --coverage",
    "compose": "(cd mongodb && docker compose up -d && bash init-db.sh)",
    "initMongo": "./mongodb/init-db.sh"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.16.13",
    "tsx": "^3.12.7",
    "typescript": "~5.4.5",
    "vitest": "^2.0.0"
  },
  "dependencies": {
    "@estuary/types": "file:../packages/estuary-types",
    "@fastify/cors": "^8.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/ws": "^8.5.13",
    "deepmerge": "^4.3.1",
    "dotenv": "^16.4.5",
    "envalid": "^8.0.0",
    "fastify": "^4.17.0",
    "mercurius": "~14.1.0",
    "mongodb": "^6.10.0"
  }
}

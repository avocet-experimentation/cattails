{
  "name": "cattails",
  "version": "1.0.0",
  "description": "Service for working with estuary's feature flags and experiments",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "npx tsx src/index.ts",
    "build": "rm -rf dist && tsup --clean --format esm ./src/index.ts",
    "start": "node dist/index.js",
    "test": "npx vitest",
    "coverage": "npx vitest run --coverage",
    "compose": "(cd mongodb && docker-compose up -d && bash init-db.sh)"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.16.13",
    "ts-node": "^10.9.2",
    "tsup": "^6.7.0",
    "tsx": "^3.12.7",
    "typescript": "^5.1.3",
    "vitest": "^0.32.0"
  },
  "dependencies": {
    "@estuary/types": "file:../packages/estuary-types",
    "@fastify/cors": "^8.0.0",
    "dotenv": "^16.4.5",
    "envalid": "^8.0.0",
    "fastify": "^4.17.0",
    "mercurius": "~14.1.0",
    "mongodb": "^6.10.0",
    "zod": "^3.23.8"
  }
}
